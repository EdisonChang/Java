keytool -genkey -alias [ALIAS NAME] -keypass [KEY_PASSWORD] -keyalg RSA -keysize 2048 -validity 1800 
-keystore [KEYSTORE_FILE_NAME] -storepass [STORE_PASSWORD] -dname 
"CN=[CURRENT NAME], OU=[ORGANIZATIONAL UNIT], O=[ORGANIZATION], L=[CITY OR LOCALITY], S=[STATE OR PROVINCE], C=[TWO-LETTER COUNTRY CODE]"
